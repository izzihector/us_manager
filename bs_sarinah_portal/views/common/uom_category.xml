<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="bs_sarinah_portal.tree_uom_category" model="ir.ui.view">
      <field name="name">UoM category Tree View</field>
      <field name="model">uom.category</field>
      <field name="inherit_id" ref="uom.product_uom_categ_tree_view"/>
      <field name="arch" type="xml">
        <field name="is_pos_groupable" position="after">
          <field name="is_available_on_portal"/>
        </field>
      </field>
    </record>
    <record id="bs_sarinah_portal.form_uom_category" model="ir.ui.view">
      <field name="name">UoM category Form View</field>
      <field name="model">uom.category</field>
      <field name="inherit_id" ref="uom.product_uom_categ_form_view"/>
      <field name="arch" type="xml">
        <field name="is_pos_groupable" position="after">
          <field name="is_available_on_portal"/>
        </field>
      </field>
    </record>
  </data>
</odoo>

                        <field name="requisition_id" string="P/A"/>
                        <field name="schedule_date"
                            string="Scheduled Date" widget="date"/>
                        <field name="product_id"/>
                        <field name="product_qty" string="Qty"/>
                        <field name="qty_ordered" string="Ordered Qty"/>
                        <field name="product_uom_id" string="UoM"/>
                        <field name="price_unit" sum="Total" widget="monetary"/>
                    </tree>
                </field>
            </field>
        </record>

        <!-- START:Purchase Request Inquiries -->
        <record id="view_replenish_request_inquiry_form" model="ir.ui.view">
            <field name="name">Purchase Request Inquiries</field>
            <field name="model">replenish.request.inquiry</field>
            <field name="inherit_id" ref="base_rmdoo.view_inquiry_report_form"/>
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <button name="list_view" position="before">
                    <button class="btn-primary" name="pick_print"
                        string="Print" type="object"/>
                    <button name="list_header_view"
                        string="List Header View" type="object"/>
                </button>
                <xpath expr="//form" position="attributes">
                    <attribute name="string">Purchase Request Inquiries</attribute>
                </xpath>
                <xpath expr="//group[label[@for='date_from']]" position="after">
                    <group>
                        <field name="replenish_request_ids"
                            options="{'no_create_edit':1}"
                            string="P/R Code" widget="many2many_tags"/>
                        <field name="confirm_uids"
                            options="{'no_create_edit':1}" widget="many2many_tags"/>
                        <field name="warehouse_ids"
                            options="{'no_create_edit':1}" widget="many2many_tags"/>
                        <field name="state"/>
                    </group>
                </xpath>
                <field name="write_uids" position="after">
                    <field name="product_ids"
                        options="{'no_create_edit':1}" widget="many2many_tags"/>
                </field>
                <xpath expr="//notebook/page[field[@name='pivot']]" position="replace"/>
                <xpath expr="//notebook/page[1]" position="before">
                    <page string="Graph">
                        <group>
                            <group string="Request per Product">
                                <field name="html" nolabel="1"/>
                            </group>
                            <group string="Request per State">
                                <field name="html2" nolabel="1"/>
                            </group>
                        </group>
                        <group>
                            <field name="pivot" nolabel="1"/>
                        </group>
                    </page>
                    <page string="Header">
                        <field name="headers" position="inside">
                            <tree decoration-muted="state=='cancel'"
                                decoration-success="state=='confirm'" decoration-warning="state=='draft'">
                                <field name="name" string="P/R"/>
                                <field name="create_uid" string="Request By"/>
                                <field name="request_department_id" string="Department"/>
                                <field name="confirm_uid"/>
                                <field name="date_ordered"
                                    string="Date Order" widget="date"/>
                                <field name="date_planned"
                                    string="Date Planned" widget="date"/>
                                <field name="warehouse_id"/>
                                <field name="state"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
                <field name="lines" position="inside">
                    <tree decoration-muted="state=='cancel'"
                        decoration-success="state=='confirm'" decoration-warning="state=='draft'">
                        <field name="replenish_request_id" string="P/R"/>
                        <field name="product_id"/>
                        <field name="date_ordered" string="Date Order" widget="date"/>
                        <field name="date_planned" string="Date Planned" widget="date"/>
                        <field name="quantity" string="Request Qty"/>
                        <field name="qty_purchase" string="Purchase Qty"/>
                        <field name="qty_received" string="Received Qty"/>
                        <field name="product_uom_id"/>
                        <field name="price_estimation" string="Est. Price"/>
                        <field name="amount_estimation"
                            string="Subtotal" sum="Total" widget="monetary"/>
                        <field name="state"/>
                    </tree>
                </field>
            </field>
        </record>

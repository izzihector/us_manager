<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- === Sequence pos custom_discount PRG ========= -->
        <record id="sequence_custom_discount" model="ir.sequence">
            <field name="name">pos_custom_discount</field>
            <field name="code">pos.custom.discount</field>
            <field name="prefix">SD/%(year)s/%(range_month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
            <field eval="True" name="use_date_range"/>
        </record>
    </data>
</odoo>